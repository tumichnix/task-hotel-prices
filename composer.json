{
    "name": "tumichnix/task-hotel-prices",
    "type": "library",
    "description": "Task Hotel prices",
    "keywords": [
        "tumichnix",
        "task-hotel-prices"
    ],
    "homepage": "https://github.com/tumichnix/task-hotel-prices",
    "license": "MIT",
    "authors": [
        {
            "name": "Hannes Daus",
            "email": "hannes.daus@gmail.com",
            "homepage": "https://hannesdaus.de",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "~7.4",
        "nesbot/carbon": "^2.29"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0",
        "squizlabs/php_codesniffer": "^3.0",
        "symfony/var-dumper": "^5.0"
    },
    "autoload": {
        "psr-4": {
            "Tumichnix\\TaskHotelPrices\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tumichnix\\TaskHotelPrices\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "check-style": "phpcs src tests",
        "fix-style": "phpcbf src tests"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "config": {
        "sort-packages": true
    }
}
